{
  "name": "create-grpc-app",
  "description": "generate node grpc application",
  "version": "1.0.1",
  "author": "Phu <ngovanphuc@hotmail.com>",
  "bin": {
    "create-grpc-app": "./bin/run"
  },
  "bugs": "https://github.com/phucngome/create-grpc-app/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "fs-extra": "^9.0.0",
    "handlebars": "^4.7.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/phucngome/create-grpc-app",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "create-grpc-app",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "phucngome/create-grpc-app",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
